@model CMS.Models.edit_ExtraEvent
@{
    ViewBag.Title = "DashBoard";
    Layout = "~/Views/Shared/Layout.cshtml";
    ViewBag.controller = "dashboard";
    ViewBag.action = "my_events";  
}

@{Html.RenderPartial("~/Views/Shared/partialDashBoard-j.cshtml");}

<div class="col-sm-10 blog-content">

    <div class="col-sm-10 text-left">

        <h1 class="text-center">Edit Extra Events</h1>

        @using (Html.BeginForm("Edit_ExtraEvent", "DashBoard", null, FormMethod.Post, new { @class = "form-horizontal" }))
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)
            <div class="form-group">
                <label class="control-label col-md-4">Title</label>
                <div class="col-md-6">
                    <input type="text" name="title" id="names" class="form-control" />
                </div>
            </div>



            <div class="form-group">
                <label class="control-label col-md-4">Discription</label>
                <div class="col-md-6">
                    <input type="text" name="discription" id="dis" class="form-control" />
                </div>
            </div>
            <div class="text-center">
                <input type="submit" class="btn btn-primary" value="Updated">
            </div>


        }
    </div>
</div>
</div>
</div>
</div>

<script>

    function info() {
        var title = '@(Model.title)';
        var dis = '@(Model.discription)';

        document.getElementById("names").value = title;
        document.getElementById("dis").value = dis;
    }

</script>


                     