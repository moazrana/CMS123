@{
    ViewBag.Title = "Contact_US";
}


<!--
testing
-->
<html>
<head>
    <link rel="stylesheet" href="~/CSS/Main.css">
    <title>Conference Mangement System</title>
</head>
<body>
    <header>
        <center><h1 style="padding-left:50px;">Conference Mangement System</h1></center>
        <img id="cms-logo" src="~/Pics/cms logo.png">
    </header>
    <nav id="nav-properties">
        <ul>
            <li>@Html.ActionLink("Home", "Index")</li>
            <li>@Html.ActionLink("SearchforConferences", "Search")</li>
            <li>@Html.ActionLink("Upcoming-Conferences", "Upcoming_Conference")</li>
            <li>@Html.ActionLink("My-Events", "myEvents")</li>
            <li>@Html.ActionLink("Contact-Us", "Contact_Us")</li>
            <li>@Html.ActionLink("About", "About")</li>
            <li id="inout">@Html.ActionLink("Signin", "signIn")</li>
            <li id="inout">@Html.ActionLink("Signup", "signup")</li>
        </ul>
    </nav>
    @*---------------------------------------------------------------------------------------*@
    <center style="margin-top:-22px;"><h1 style="background-color:rgb(215,215,215);padding:10px;">Contact Us! Feel free to say Hello...!</h1></center>
    <div class="login">
        <table>
            </tr>
            <tr>
                <td>
                    <label for="first_name">First Name *</label>
                </td>
                <td>
                    <input type="text" name="first_name" maxlength="50" size="30">
                </td>
            </tr>

            <tr>
                <td>
                    <label for="last_name">Last Name *</label>
                </td>
                <td>
                    <input type="text" name="last_name" maxlength="50" size="30">
                </td>
            </tr>
            <tr>
                <td>
                    <label for="email">Email Address *</label>
                </td>
                <td>
                    <input type="text" name="email" maxlength="80" size="30">
                </td>
            </tr>
            <tr>
                <td>
                    <label for="telephone">Telephone Number</label>
                </td>
                <td>
                    <input type="text" name="telephone" maxlength="30" size="30">
                </td>
            </tr>
            <tr>
                <td>
                    <label for="comments">Comments *</label>
                </td>
                <td>
                    <textarea name="comments" maxlength="1000" cols="25" rows="6"></textarea>
                </td>

            </tr>
            <tr>
                <td colspan="2" style="text-align:center;">
                    <!-- We are grateful to you for keeping this link in place. thank you. -->
                    <input type="button" value="Submit">
                </td>
            </tr>
        </table>
    </div>
</body>
</html>




